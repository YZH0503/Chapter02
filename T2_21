#include <iostream>

using namespace std;

int main()
{
    for(int i=1;i<=9;i++)
    {
        if(i==1||i==9)
        {
            cout<<"*********"<<endl;
        }
        else
        {
               cout<<"*       *"<<endl;
        }
    }
    cout<<"================"<<endl;
    for(int j=1;j<=9;j++)
    {
        if(j==1||j==9)
        {
            cout<<"   ***   "<<endl;
        }
        else if(j==2||j==8)
        {
            cout<<" *     * "<<endl;
        }
        else
        {
            cout<<"*       *"<<endl;
        }
    }
    cout<<"================"<<endl;

    for(int i=1;i<=3;i++)
    {
        for(int j=1;j<=3-i;j++)
          cout<<" ";

        for(int k=1;k<=2*i-1;k++)
        cout<<"*";
        cout<<endl;
    }
    for(int h=1;h<=6;h++)
    {
        cout<<"  *  "<<endl;
    }
    cout<<"=================="<<endl;
    cout<<"    *    "<<endl;
    cout<<"   * *   "<<endl;
    cout<<"  *   *  "<<endl;
    cout<<" *     * "<<endl;
    cout<<"*       *"<<endl;
    cout<<" *     * "<<endl;
    cout<<"  *   *  "<<endl;
    cout<<"   * *   "<<endl;
    cout<<"    *    "<<endl;
}
