#include <iostream>

using namespace std;

int main()
{

    int a,b;
    cout<<"please input 2 numbers";
    cin>>a>>b;
    if(a>b){cout<<a<<"is large";}
      else if(a<b){cout<<b<<"is large";}
        else if(a==b){cout<<"these numbers are equal";}

    return 0;
}



