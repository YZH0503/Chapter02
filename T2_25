#include <iostream>

using namespace std;

int main()
{
    int a,b,c;
    cin>>a>>b;
    c=a%b;
    if(c==0)
    {
        cout<<"yes"<<endl;

    }
    else
    {
        cout<<"no"<<endl;
    }
    return 0;
}
