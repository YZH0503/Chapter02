#include <iostream>

using namespace std;

int main()
{
    char a,b,c,d,e;
    cin>>a>>b>>c>>d>>e;
    cout<<a<<"   "<<b<<"   "<<c<<"   "<<d<<"   "<<e;
    return 0;
}
