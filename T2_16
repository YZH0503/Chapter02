#include <iostream>ss

using namespace std;

int main()
{
   int a,b,c,d,e,f;
   cout<<"please output 2 numbers";
   cin>>a>>b;
   c=a+b;
   d=a*b;
   e=a-b;
   f=a/b;
   cout<<"the sum is "<<c<<";the product is "<<d<<";the difference is "<<e<<";the quotient is "<<f<<endl;
    return 0;
}

